    "@reduxjs/toolkit": "^2.5.0",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.468.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.0.2",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "description": "The package where lives the boilerplate project.",
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.16.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "env-cmd": "^10.1.0",
    "eslint": "^9.16.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-exports": "^0.9.1",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.11",
    "msw": "^2.6.8",
    "postcss": "^8.4.49",
    "prettier": "3.4.2",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.16",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.18.0",
    "vite": "^6.0.3",
    "vite-plugin-eslint2": "^5.0.3",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "boilerplate",
    "frontend",
    "frontend-react",
    "web-development"
  ],
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "name": "boilerplate",
  "private": true,
  "scripts": {
    "build": "rimraf dist && tsc -b && vite build",
    "build:dev": "env-cmd -f .env.dev npm run build",
    "build:local": "env-cmd -f .env.local npm run build",
    "build:prod": "env-cmd -f .env.prod npm run build",
    "format": "prettier --write .",
    "format:lint": "npm run format && npm run lint",
    "lint": "eslint src",
    "prepare": "husky",
    "preview": "vite preview",
    "start": "vite",
    "start:dev": "env-cmd -f .env.dev npm run start",
    "start:local": "env-cmd -f .env.local npm run start",
    "start:prod": "env-cmd -f .env.prod npm run start",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "npm run test --coverage",
    "test:watch": "vitest --watch --passWithNoTests"
  },
  "type": "module",
  "version": "1.0.0"
}
